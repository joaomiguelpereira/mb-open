<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:mbw="urn:import:com.medibooking.admin.client.view.widget">
	<ui:with type="com.medibooking.admin.client.view.resources.GlobalResources"
		field="res"></ui:with>

	<g:HTMLPanel ui:styleName="{res.editorCss.editorContainer}">
		<table border="0" class="{res.editorCss.editorTable}">
			<tr>
				<td class="{res.editorCss.labelContainer}">
					<g:Label>Email:</g:Label>
				</td>

				<td class="{res.editorCss.leftAligned}">
					<mbw:TextBoxEditor ui:field="email" />
				</td>
			</tr>


			<tr>
				<td class="{res.editorCss.labelContainer}">
					<g:Label>Password:</g:Label>
				</td>
				<td class="{res.editorCss.leftAligned}">

					<mbw:PasswordTextBoxEditor ui:field="password" />
				</td>
			</tr>

			<tr>
				<td class="{res.editorCss.labelContainer}">
				</td>
				<td class="{res.editorCss.leftAligned} {res.editorCss.buttonArea} ">
					<g:Button ui:field="submit">Autenticar</g:Button>
				</td>
			</tr>

		</table>

	</g:HTMLPanel>
</ui:UiBinder> 